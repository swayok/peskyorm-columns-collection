{
    "name": "swayok/peskyorm-columns-collection",
    "type": "library",
    "description": "Table structure columns and traits collection for PeskyORM",
    "keywords": [
        "php"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/peskyorm-columns-collection/issues"
    },
    "autoload": {
        "psr-4": {
            "PeskyORMColumns\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "swayok/peskyorm": "v3.x-dev",
        "swayok/utils": "^1.7.1",
        "ramsey/uuid": "*"
    },
    "suggest": {
        "symfony/http-foundation": "Needed to files and images columns"
    },
    "require-dev": {
        "symfony/http-foundation": ">=3.0"
    }
}
